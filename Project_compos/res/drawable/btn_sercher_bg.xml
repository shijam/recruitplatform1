<?xml version="1.0" encoding="utf-8"?>
<inset android:insetLeft="0.0px" android:insetRight="0.0px" android:insetTop="0.0px" android:insetBottom="0.0px"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <selector>
        
        <item android:state_focused="true">
            <shape>
                <gradient android:startColor="@color/base_start_color" android:endColor="@color/base_start_color" android:angle="45.0" />
                <corners android:radius="1.0dip" />
            </shape>
        </item>
        <item android:state_pressed="true">
            <shape>
                <gradient android:startColor="@color/base_start_color" android:endColor="@color/base_start_color" android:angle="45.0" />
                <corners android:radius="1.0dip" />
            </shape>
        </item>
         
        <item>
            <shape>
                <stroke android:width="1.0px" android:color="@color/rounded_container_border" />
                <gradient android:startColor="@color/whites" android:endColor="@color/whites" android:angle="45.0" />
                <corners android:radius="1.0dip" />
            </shape>
        </item>
    </selector>
</inset>